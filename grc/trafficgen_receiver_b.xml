<?xml version="1.0"?>
<block>
	<name>receiver_b</name>
	<key>trafficgen_receiver_b</key>
	<category>[trafficgen]</category>
	<import>import trafficgen</import>
	<make>trafficgen.receiver_b($use_acks)</make>
	<param>
		<name>Use ACKs</name>
		<key>use_acks</key>
		<value>True</value>
		<type>bool</type>
		<option>
			<name>Yes</name>
			<key>True</key>
		</option>
		<option>
			<name>No</name>
			<key>False</key>
		</option>
	</param>
	<sink>
		<name>pdu</name>
		<type>message</type>
	</sink>
</block>
